{
  "auto-close-tag.fullMode": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSurroundingLines": 5,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorWidth": 2,
  "editor.dragAndDrop": false,
  "editor.experimental.asyncTokenization": true,
  "editor.experimental.asyncTokenizationLogging": true,
  "editor.experimental.asyncTokenizationVerification": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.find.globalFindClipboard": true,
  "editor.fontFamily": "'Operator Mono SSm', 'Input Mono', 'Fira Code', Hack, Monaco, Consolas, monospace",
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.matchBrackets": "near",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    ".vimrc": "viml",
    "bash": "shellscript"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.external.osxExec": "iterm.app",
  "terminal.integrated.env.osx": {},
  "window.closeWhenEmpty": false,
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.splitInGroupLayout": "vertical",
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.tabSizing": "fixed",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "workbench.preferredHighContrastColorTheme": "Material Theme Darker High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "Material Theme Lighter High Contrast",
  "workbench.preferredLightColorTheme": "Material Theme Lighter",
  "workbench.preferredLightColorTheme": "Tokyo Night Light",
  "workbench.quickOpen.preserveInput": true,
  "workbench.settings.useSplitJSON": true,
  "workbench.welcomePage.experimental.videoTutorials": "on",
  "explorer.confirmDelete": false,
  "files.trimFinalNewlines": true,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "redhat.telemetry.enabled": true,
  "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
}
